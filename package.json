  "files": [
    "dist"
  ],
  "keywords": [
    "ðŸ¥"
  ],
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "boeing": "^0.2.1",
    "husky": "^4.2.5",
    "inject-ga": "^0.2.4",
    "lint-staged": "^10.2.11",
    "marshmallow": "^0.8.0",
    "parcel": "^1.12.4",
    "prettier-standard": "^16.4.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "scripts": {
    "build": "tsdx build --entry index.tsx",
    "deploy": "marshmallow -o docs -i artwork.psd -f -m && inject-ga docs/index.html -o docs/index.html -c 'UA-24389952-13' && boeing docs && rimraf docs",
    "dev": "tsdx watch --entry index.tsx && parcel example.html",
    "lint": "prettier-standard --check",
    "server": "parcel example.html",
    "test": "tsdx test --env=jsdom"
  },
  "engines": {
    "node": "^12.18.0",
    "npm": "^6.14.5"
  },
  "prettier": {
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier-standard --format",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "transform-async-to-promises"
    ]
  }
